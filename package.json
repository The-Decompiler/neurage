{
	"private": true,
	"name": "neurage",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"client": "yarn workspace @neurage/client dev",
		"server": "python3 packages/server/app/main.py"
		"start": "concurrently --kill-others-on-fail \"yarn client\" \"yarn server\""
	},
	"dependencies": {
		"concurrently": "^6.0.2"
	}
}
